{
  "name": "gatsby-plugin-cdn-files",
  "version": "1.0.0",
  "description": "Pull specified filetypes from remote system, downloads them into the static folder and moves them into a public folder for access on in the application.",
  "main": "gatsby-node.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "Thrillworks/gatsby-plugin-cdn-files.git"
  },
  "keywords": [
    "plugin",
    "gatsby",
    "cdn",
    "gatsby-plugin",
    "download",
    "remote"
    "gif",
    "png"
  ],
  "author": "Thrillworks Inc",
  "license": "ISC",
  "bugs": {
    "url": "Thrillworks/gatsby-plugin-cdn-files/issues"
  },
  "homepage": "Thrillworks/gatsby-plugin-cdn-files#readme",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.1.4"
  },
  "dependencies": {
    "fs-extra": "^8.0.1",
    "gatsby-source-filesystem": "^2.1.0",
    "lodash": "^4.17.11"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
